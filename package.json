{
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/@(default)"
  ],
  "scripts": {
    "postinstall": "lerna run build:cjs --scope react-vision-*",
    "start": "yarn storybook",
    "build": "lerna run build --scope react-*",
    "docs": "jsdoc2md --no-cache --template ./doc/jsdoc2md/README.hbs --partial ./doc/jsdoc2md/body.hbs ./doc/jsdoc2md/themes.hbs --files ./packages/react-vision-core/src/widgets/* ./packages/react-vision-dom/src/widgets/* ./packages/react-vision-dom-maps/src/* --configure ./doc/jsdoc2md/jsdoc2md.json > ./doc/widgets/README-generated.md",
    "watch": "lerna run watch --scope react-vision-* --parallel",
    "release": "scripts/release.sh",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix .",
    "type-check": "tsc",
    "type-check:watch": "yarn type-check --watch",
    "test": "yarn jest --reporters=default",
    "test:watch": "yarn jest --watch --reporters=default",
    "test:e2e": "yarn test:e2e:local",
    "test:e2e:local": "NODE_ENV=test wdio wdio.local.conf.js",
    "test:e2e:saucelabs": "wdio wdio.saucelabs.conf.js",
    "test:integration": "NODE_ENV=development yarn test:regressions && yarn test:argos",
    "test:examples": "node scripts/test-examples.js",
    "test:build": "yarn build && yarn test:size",
    "test:regressions": "webpack --config integration/webpack.config.js && node integration/runTest.js",
    "test:argos": "argos upload integration/screenshots --token $ARGOS_TOKEN || true",
    "test:size": "bundlesize",
    "storybook": "start-storybook -p 6006 -c storybook -s storybook/public",
    "storybook:build": "build-storybook -s storybook/public -o website/storybook -c storybook",
    "website:build": "yarn build && yarn webpack --config website/webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@storybook/addon-a11y": "5.0.11",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/react": "5.0.11",
    "@storybook/theming": "5.0.11",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/googlemaps": "^3.30.16",
    "@types/jest": "^24.0.0",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "@wdio/cli": "5.11.14",
    "@wdio/jasmine-framework": "5.12.1",
    "@wdio/local-runner": "5.12.1",
    "@wdio/sauce-service": "5.12.1",
    "@wdio/selenium-standalone-service": "5.12.1",
    "@wdio/spec-reporter": "5.12.1",
    "@wdio/static-server-service": "5.12.1",
    "argos-cli": "0.1.2",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "bundlesize": "0.18.0",
    "cliniasearch": "1.0.0",
    "conventional-changelog-cli": "2.0.23",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.2.2",
    "eslint-config-clinia": "^1.0.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.16.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
