    "@wdio/local-runner": "^5.15.1",
    "@wdio/sauce-service": "^5.14.0",
    "@wdio/selenium-standalone-service": "^5.15.0",
    "@wdio/spec-reporter": "^5.15.1",
    "@wdio/static-server-service": "^5.14.4",
