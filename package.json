    "jest": "24.9.0",
    "jest-junit": "^9.0.0",
    "jest-watch-typeahead": "0.4.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.2",
    "json": "9.0.6",
    "lerna": "3.16.4",
    "lodash.orderby": "4.6.0",
    "mversion": "1.13.0",
    "null-loader": "3.0.0",
    "prettier": "1.18.2",
    "prop-types": "15.6.2",
    "puppeteer": "1.20.0",
    "react": "16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "16.8.6",
    "react-native": "0.58.4",
    "react-test-renderer": "16.8.6",
    "rheostat": "2.2.0",
    "rollup": "1.20.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-uglify": "6.0.3",
    "style-loader": "1.0.0",
    "typescript": "3.5.3",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.7"
  },
  "bundlesize": [
    {
      "path": "packages/react-vision/dist/umd/Core.min.js",
      "maxSize": "3 kB"
    },
    {
      "path": "packages/react-vision/dist/umd/Connectors.min.js",
      "maxSize": "21 kB"
    },
    {
      "path": "packages/react-vision/dist/umd/Dom.min.js",
      "maxSize": "33.25 kB"
    },
    {
      "path": "packages/react-vision-core/dist/umd/ReactVisionCore.min.js",
      "maxSize": "27 kB"
    },
    {
      "path": "packages/react-vision-dom/dist/umd/ReactVisionDOM.min.js",
      "maxSize": "35.50 kB"
    },
    {
      "path": "packages/react-vision-dom-maps/dist/umd/ReactVisionDOMMaps.min.js",
      "maxSize": "7 kB"
    }
  ]
}
