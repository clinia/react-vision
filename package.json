    "webpack-cli": "^3.3.9"
  },
  "bundlesize": [
    {
      "path": "packages/react-vision/dist/umd/Core.min.js",
      "maxSize": "2.75 kB"
    },
    {
      "path": "packages/react-vision/dist/umd/Connectors.min.js",
